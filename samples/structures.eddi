struct Complex {
    int imag;
    int real;
    float d;
}

void main(){
    println("Structures test");

    Complex c;
    c.imag = 222;
    c.real = 666;
    c.d = 3.23;

    println(c.imag);
    println(c.real);
    println(c.d);

    c.imag += 111;
    c.real += 222;
    c.d += 1.1;

    println(c.imag);
    println(c.real);
    println(c.d);

    test(c);
}

void test(Complex a){
    println(a.imag);
    println(a.real);
    println(a.d);
}
