void main(){
    println(sum(0));
    println(sum(4));
    println(sum(5));

    println(factorial(9));

    println(hello());
    println(hello());
}

int sum(int a){
    if(a == 0){
        return 0;
    }

    return a + sum(a - 1);
}

int factorial(int n){
    if(n == 0){
        return 1;
    }

    return n * factorial(n - 1);
}

