void main(){
    int a = 44;
    test(&a);

    print(a);
    print("|");
}

void test(int* a){
    print(*a);
    print("|");

    int* b = a;
    
    print(*b);
    print("|");
    
    *a = 55; 

    print(*a);
    print("|");
    print(*b);
    print("|");
    
    *b = 66;
    
    print(*b);
    print("|");
    print(*a);
    print("|");
}

