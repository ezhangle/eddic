void main(){
    str a = "a";
    test(&a);

    print(a);
    print("|");
}

void test(str* a){
    print(*a);
    print("|");

    str* b = a;
    
    print(*b);
    print("|");
    
    *a = "b"; 

    print(*a);
    print("|");
    print(*b);
    print("|");
    
    *b = "c";
    
    print(*b);
    print("|");
    print(*a);
    print("|");
}
