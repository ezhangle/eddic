struct Counter {
    int value;

    void increment(){
        this.value = this.value + 1;

        println(this.value);
    }

    void add(int number){
        this.value = this.value + number;
    }
    
    void add(int n1, int n2){
        this.value = this.value + n1;
        this.value = this.value + n2;
    }
    
    void addSub(int n1, int n2){
        this.add(n1);
        this.add(n2);
    }
}

void main(){
    Counter counter;
    counter.increment();
    println(counter.value);
    counter.add(99);
    println(counter.value);
    counter.add(11, 69);
    println(counter.value);
    counter.addSub(11, 69);
    println(counter.value);
}
