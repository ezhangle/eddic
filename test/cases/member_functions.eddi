struct Counter {
    int value;

    void increment(){
        value = value + 1;
    }

    void add(int number){
        value = value + number;
    }
    
    void add(int n1, int n2){
        value = value + n1;
        value = value + n2;
    }
    
    void addSub(int n1, int n2){
        this.add(n1);
        this.add(n2);
    }
}

void main(){
    Counter counter;
    print(counter.value);
    print("|");
    counter.increment();
    print(counter.value);
    print("|");
    counter.add(99);
    print(counter.value);
    print("|");
    counter.add(11, 69);
    print(counter.value);
    print("|");
    counter.addSub(11, 69);
    print(counter.value);
    print("|");
}
