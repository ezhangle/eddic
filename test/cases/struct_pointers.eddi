struct A {
    int a;
    string b;
    float c;
}

void main(){
    A a;

    a.a = 44;
    a.b = "44";
    a.c = 44f;

    test(a);

    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");
}

void test(A* a){
    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");

    A* b = a;
    
    print(b.a);
    print("|");
    print(b.b);
    print("|");
    print(b.c);
    print("|");
    
    a.a = 55;
    a.b = "55";
    a.c = 55f;

    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");
    print(b.a);
    print("|");
    print(b.b);
    print("|");
    print(b.c);
    print("|");
    
    b.a = 66;
    b.b = "66";
    b.c = 66f;
    
    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");
    print(b.a);
    print("|");
    print(b.b);
    print("|");
    print(b.c);
    print("|");
}
