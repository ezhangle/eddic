include <print>

void main(){
    int a = 2;
    int b = 3;

    a += 4;
    print(a);
    print("|");

    a += b;
    print(a);
    print("|");

    a -= b;
    print(a);
    print("|");

    a *= b;
    print(a);
    print("|");

    a /= b;
    print(a);
    print("|");

    a %= b;
    print(a);
    print("|");
}
