struct A {
    int a;

    this(){
        this.a = 0;
        
        print("CA");
        print("|");
    }

    this(int a){
        this.a = a;

        print("CAI");
        print("|");
    }

    ~this(){
        print("DA");
        print("|");
    }

    void debug(){
        print(this.a);
        print("|");
    }
}

void main(){
    A* a = new A();
    a.debug();
    delete a;
}
