int ga = 100;

struct A {
    int a;

    this(){
        this.a = 0;
        
        print("CA");
        print("|");
    }

    this(string a){
        print("CA");
        print("|");
        
        print(a);
        print("|");
    }

    this(int a){
        this.a = a;

        print("CAI");
        print("|");
    }

    this(int a, int b){
        this.a = a * b;
        
        print("CAII");
        print("|");
    }

    ~this(){
        print("DA");
        print("|");
    }

    void debug(){
        print(this.a);
        print("|");
    }
}

void main(){
    A* a = new A();
    a.debug();
    delete a;

    A* b = new A(55);
    b.debug();
    delete b;

    A* c = new A(33, ga);
    c.debug();
    delete c;

    A* d = new A("666");
    delete d;
}
