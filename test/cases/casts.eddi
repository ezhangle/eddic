void main(){
    int a = 5;
    float b = (float) a;
    float c = 333.879877;
    float d = (float) c;

    print(b);
    print("|");
    print((int) b);
    print("|");
    print((int) 4.33);
    print("|");
    print((int) c);
    print("|");
    print((float) a);
    print("|");

    float e = (float) a + 3.33;
    print(e);
    print("|");

    test_char_casts();
}

void debug(int c){
    print(c);
    print("|");
}

void debug(char c){
    print(c);
    print("|");
}

void test_char_casts(){
    int a = 66;
    
    char c = (char) a;
    char d = (char) c;

    debug(c);
    debug(d);

    int e = (int) 'Z';
    debug(e);
}
