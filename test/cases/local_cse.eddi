include<print>

int ga = 9;
int gb = 99;
int gc = 999;

void main(){
    test_a();
    test_b();
}

void test_a(){
    int a = ga * 99;
    int b = 99 * ga;
    int c = ga * 99;
    int d = 99 * 99;

    print(a);
    print("|");
    print(b);
    print("|");
    print(c);
    print("|");
    print(d);
    print("|");
}

void test_b(){
    int a = gb * gc;
    int b = gc * gb;
    int c = gb * gc;

    print(a);
    print("|");
    print(b);
    print("|");
    print(c);
    print("|");
}
