//Struct with a copy constructor
struct With {
    int a;
    int b;

    this(With* rhs){
        a = rhs.a;
        b = rhs.b;
    }
}

void main(){
    With a;
    a.a = 9;
    a.b = 5;

    With b = a;

    print(b.a);
    print("|");
    print(b.b);
    print("|");

    a.a = 99;
    a.b = 55;

    With c;
    c = a;
    
    print(c.a);
    print("|");
    print(c.b);
    print("|");
}
