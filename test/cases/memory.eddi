struct A {
    int a;
    int b;
    int c;
}

void main(){
    A a;

    a.a = 555;
    a.b = 666;
    a.c = 777;

    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");

    test_ptr(a);
    test_value(a);
}

void test_ptr(A* a){
    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");
}

void test_value(A a){
    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");
}
