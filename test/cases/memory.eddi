string ga = "4";

struct A {
    int a;
    string b;
    int c;
    string d;
}

void main(){
    print(ga);
    print("|");

    test_ptr(ga);
    test_value(ga);

    string b = "1";

    print(b);
    print("|");

    test_ptr(b);
    test_value(b);

    A a;

    a.a = 5;
    a.b = "6";
    a.c = 7;
    a.d = "8";

    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");
    print(a.d);
    print("|");

    test_ptr(a);
    test_value(a);
}

void test_ptr(string* a){
    print(*a);
    print("|");
}

void test_value(string a){
    print(a);
    print("|");
}

void test_ptr(A* a){
    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");
    print(a.d);
    print("|");
}

void test_value(A a){
    print(a.a);
    print("|");
    print(a.b);
    print("|");
    print(a.c);
    print("|");
    print(a.d);
    print("|");
}
