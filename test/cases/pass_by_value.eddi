include<print>

struct A {
    int a;

    this(){
        print("C1");
        print("|");
    }

    this(A* rhs){
        print("C2");
        print("|");
        a = rhs.a;
    }
}

void main(){
    A a;
    a.a = 9;

    print(a.a);
    print("|");

    test(a);
}

void test(A a){
    print(a.a);
    print("|");
}
