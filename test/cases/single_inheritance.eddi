struct A {
    float a;
    int a_a;
    
    void init_a(){
        a = 55.2;
        a_a = 55;
    }

    void test_a(){
        print(a);
        print("|");
        
        print(a_a);
        print("|");

        a += 1.1;
    }
}

struct B extends A {
    char a;
    int a_b;
    
    void init_b(){
        a = 'B';
        a_b = 66;
        init_a();
    }

    void test_b(){
        print(a);
        print("|");
        
        print(a_a);
        print("|");
        print(a_b);
        print("|");

        test_a();
    }
}

struct C extends B {
    int a;
    int a_c;

    void init(){
        a = 99;
        a_c = 77;
        init_b();
    }

    void test(){
        print(a);
        print("|");
        
        print(a_a);
        print("|");
        print(a_b);
        print("|");
        print(a_c);
        print("|");

        test_b();
        test_a();
    }
}

void main(){
    C c;

    c.init();
    c.test();
    c.test_b();
    c.test_a();
        
    print(c.a_a);
    print("|");
    print(c.a_b);
    print("|");
    print(c.a_c);
    print("|");
}
